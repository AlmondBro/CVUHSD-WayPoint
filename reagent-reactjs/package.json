        "devtron": "^1.4.0",
        "electron-react-devtools": "^0.5.3",
        "eslint-plugin-react": "^7.7.0",
        "foreman": "^3.0.0",
        "serve": "^6.5.3"
    },
    "presets": [
        "env"
    ],
    "keywords": [],
    "author": "Juan David Lopez Londono and Kevin Erkelenz",
    "homepage": "./",
