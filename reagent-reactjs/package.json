{
    "name": "reagent-reactjs",
    "description": "The CVUHSD reagent is a web app where teachers can quickly access functions such as submitting a helpdesk ticket, calling the helpdesk, accessing the troubleshootign tutorials, or viewing announcements.",
    "version": "0.1.0",
    "private": true,
    "main": "src/electron-starter.js",
    "dependencies": {
        "cors": "^2.8.4",
        "cors-anywhere": "^0.4.1",
        "electron-is-dev": "^0.3.0",
        "express": "^4.16.3",
        "fetch-timeline": "^1.1.4",
        "fullname": "^3.3.0",
        "jquery": "^3.3.1",
        "jsx-to-string": "^1.4.0",
        "macaddress": "^0.2.9",
        "node-run-cmd": "^1.0.1",
        "oauth": "^0.9.15",
        "oauth-1.0a": "^2.2.4",
        "os": "^0.1.1",
        "prop-types": "^15.6.2",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-scripts": "1.1.1",
        "request": "^2.87.0",
        "sendmail": "^1.4.1",
        "twitter": "^1.7.1",
        "undefsafe": "^2.0.2"
    },
    "scripts": {
        "start": "set BROWSER=nul && react-scripts start",
        "server": "node src/server.js",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "electron": "electron .",
        "dev": "nf start -p 3000",
        "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
        "real": "concurrently \"npm run server\" \"npm run dev\"",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "ebuild": "npm run build && node_modules/.bin/build",
        "build-pack": "npm run build && npm run pack",
        "build-dist": "npm run build && npm run dist",
        "electron-pack": "build --em.main=build/electron.js",
        "preelectron-pack": "npm build"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "concurrently": "^3.6.1",
        "devtron": "^1.4.0",
        "electron": "^2.0.5",
        "electron-builder": "^20.26.0",
        "electron-react-devtools": "^0.5.3",
        "eslint-plugin-react": "^7.7.0",
        "foreman": "^3.0.0",
        "serve": "^6.5.3",
        "wait-on": "^2.1.0"
    },
    "presets": [
        "env"
    ],
    "keywords": [],
    "author": "Juan David Lopez Londono and Kevin Erkelenz",
    "homepage": "./",
    "proxy": "http://localhost:4000",
    "build": {
        "extends": null,
        "productName": "Centinela WayPoint",
        "appId": "com.waypoint",
        "files": [
            "build/**/*",
            "src/*"
        ],
        "win": {
            "icon": "build/img/wp-icon-grey-256x256.ico"
        }
    }
}
